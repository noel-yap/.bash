[alias]
	aliases = "config --get-regexp alias"
	destash = "stash pop"
	clone-or-pull = "!git-clone-or-pull.sh"
	get = "!git-get.sh"
	graft = "!git-graft.sh"
	graph = log --graph --branches --remotes --tags  --format=format:'%Cgreen%h %Cresetâ€¢ %<(75,trunc)%s (%cN, %cr) %Cred%d' --date-order
	push-pull = "!while ! git push; do git pull; done"
	reap = "!git-reap.sh"
	sow = push
	sprout = "!git-sprout.sh"
	uncommit = "reset HEAD^ --"
	unstage = "reset -q HEAD --"
	upstream-set = "!github-upstream-set.sh"
[user]
	name = Noel Yap
	email = noel.yap+github.com@gmail.com
[pull]
	rebase = true
[push]
	default = current
[fetch]
	prune = true
[merge]
	tool = meld
[diff]
	colorMoved = zebra
	renameLimit = 16384
[log]
	date = iso-strict-local
[core]
	editor = vi
	excludesfile = ~/.gitignore-global
[init]
	defaultBranch = master
[http]
	postBuffer = 524288000
[url "ssh://git@gitlab.com"]
	insteadOf = https://gitlab.com
